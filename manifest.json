{
  "manifest_version": 2,

  "name": "ChatBox",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Chat?"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
